#include <iostream>

using namespace std

void print_numbers(int n) {
    if (n > 1) {
        print_numbers(n-1);
    }
  cout << n << endl;
}
int factorial(int n) {
    if (n == 1) {
        return 1;
    } else {
        return n * factorial(n-1);
    }

int main() {
   
    
    print_numbers(50);
    factorial(6);
}
